{
  "name": "yiitk/yiitk",
  "description": "YiiTK - Yii Framework version 2 Toolkit",
  "type": "library",
  "keywords": [
    "yii2",
    "extension",
    "framework",
    "php"
  ],
  "license": "BSD-3-Clause",
  "version": "0.0.6",
  "authors": [
    {
      "name": "YiiTK",
      "email": "yiitk@jsas.com.br"
    },
    {
      "name": "Jonatas Sas",
      "email": "atendimento@jsas.com.br"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0.0",

    "yiisoft/yii2": "~2.0.15",

    "yiisoft/yii2-queue": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.0.7",

    "php-curl-class/php-curl-class": "~7.4.0",

    "kartik-v/yii2-krajee-base": "1.8.9",
    "kartik-v/yii2-password": "1.5.3",
    "kartik-v/yii2-date-range": "1.6.9",
    "kartik-v/yii2-dropdown-x": "1.2.0",
    "kartik-v/yii2-money": "1.2.2",
    "kartik-v/yii2-widget-datepicker": "1.4.3",
    "kartik-v/yii2-widgets": "3.4.0",
    "kartik-v/yii2-field-range": "1.3.2",

    "2amigos/yii2-file-upload-widget": "1.0.8",

    "erusev/parsedown": "~1.6.4",
    "michelf/php-markdown": "~1.8.0",

    "donatj/phpuseragentparser": "~0.5.3",
    "mobiledetect/mobiledetectlib": "~2.8.31",

    "nesbot/carbon": "~1.9.0"
  },
  "require-dev": {
    "yiisoft/yii2-gii": "~2.0.0",
    "yiisoft/yii2-faker": "~2.0.0",

    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1"
  },
  "autoload": {
    "psr-4": {
      "yiitk\\": "src/toolkit"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/yiitk/yiitk"
    }
  ]
}
